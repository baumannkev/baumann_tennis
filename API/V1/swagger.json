{
    "swagger": "2.0",
    "info": {
        "description": "Simple API for Baumann Tennis Academy",
        "version": "1",
        "title": "BaumannTennis API Documentation"
    },
    "tags": [{
        "name": "players",
        "description": "Access to players"
    }],
    "paths": {
        "/players": {
            "get": {
                "tags": [
                    "players"
                ],
                "operationId": "getPlayersV1",
                "summary": "Gets all players",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "200 OK",
                        "examples": {
                            "id": 1,
                            "name": "John Smith",
                            "age": "22",
                            "skill": "intermediate"
                        }
                    },
                    "404": {
                        "description": "404 Not Found"
                    }
                }
            }
        },
        "/players{playerID}": {
            "get": {
                "tags": [
                    "players"
                ],
                "operationId": "getPlayerV1",
                "summary": "Gets a specific player",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "200 OK",
                        "examples": {
                            "id": 2,
                            "name": "Sally Smith",
                            "age": "20",
                            "skill": "beginner"
                        }
                    },
                    "404": {
                        "description": "404 Not Found"
                    }
                }
            }
        }
    }
}